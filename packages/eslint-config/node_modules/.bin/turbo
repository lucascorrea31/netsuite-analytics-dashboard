#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lucas-dev/development/personal/netsuite/netsuite-analytics-dashboard/my-turborepo/node_modules/.pnpm/turbo@2.6.1/node_modules/turbo/bin/node_modules:/Users/lucas-dev/development/personal/netsuite/netsuite-analytics-dashboard/my-turborepo/node_modules/.pnpm/turbo@2.6.1/node_modules/turbo/node_modules:/Users/lucas-dev/development/personal/netsuite/netsuite-analytics-dashboard/my-turborepo/node_modules/.pnpm/turbo@2.6.1/node_modules:/Users/lucas-dev/development/personal/netsuite/netsuite-analytics-dashboard/my-turborepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lucas-dev/development/personal/netsuite/netsuite-analytics-dashboard/my-turborepo/node_modules/.pnpm/turbo@2.6.1/node_modules/turbo/bin/node_modules:/Users/lucas-dev/development/personal/netsuite/netsuite-analytics-dashboard/my-turborepo/node_modules/.pnpm/turbo@2.6.1/node_modules/turbo/node_modules:/Users/lucas-dev/development/personal/netsuite/netsuite-analytics-dashboard/my-turborepo/node_modules/.pnpm/turbo@2.6.1/node_modules:/Users/lucas-dev/development/personal/netsuite/netsuite-analytics-dashboard/my-turborepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/turbo@2.6.1/node_modules/turbo/bin/turbo" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/turbo@2.6.1/node_modules/turbo/bin/turbo" "$@"
fi
